    enable_auto_tool_choice: true
    trust_remote_code: true
    tool_call_parser: qwen3_moe
    reasoning_parser: qwen3_moe

  - name: qwen3_4b
    model_path: mlx-community/Qwen3-4B-Instruct-2507-8bit
