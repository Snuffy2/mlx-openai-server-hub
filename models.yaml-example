    max_concurrency: 1
    port: 5005
    enable_auto_tool_choice: true
    trust_remote_code: true
    tool_call_parser: qwen3
    reasoning_parser: qwen3

  - name: deepseek_qwen_14b
    model_path: mlx-community/Unsloth-DeepSeek-R1-Distill-Qwen-14B-4bit
